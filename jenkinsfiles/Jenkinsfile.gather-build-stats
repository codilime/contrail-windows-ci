library "contrailWindows@$BRANCH_NAME"

def cmd

pipeline {
    agent none
    stages {
        stage('Gather build stats') {
            agent { label 'ansible' }
            environment {
                MYSQL = credentials('monitoring-mysql')
            }
            steps {
                deleteDir()

                sh "curl ${MONITORED_BUILD_URL}/wfapi/describe"
            }
        }
    }
}
